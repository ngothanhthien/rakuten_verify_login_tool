DATABASE_URL="file:./infrastructure/db/prisma/dev.db"

# Parallel Processing Configuration
# Number of concurrent workers for credential verification (default: 6)
CREDENTIAL_CHECK_CONCURRENCY=6

# Number of credentials each worker processes per batch (default: 3)
CREDENTIAL_CHECK_BATCH_SIZE=3

# Polling interval in milliseconds when no credentials are available (default: 1000)
CREDENTIAL_CHECK_POLLING_INTERVAL_MS=1000

# Timeout in minutes for stale claims - claims older than this will be released (default: 10)
CREDENTIAL_CHECK_STALE_TIMEOUT_MINUTES=10

AUTOMATE_DEBUG=true
